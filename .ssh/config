# GitHub Configuration
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519_github_auth
  IdentitiesOnly yes

# Gitea Configuration (git.artichoke.cc)
Host git.artichoke.cc
  HostName git.artichoke.cc
  User git
  IdentityFile ~/.ssh/id_ed25519_gitea_artichokecc_auth
  IdentitiesOnly yes

AddKeysToAgent yes
